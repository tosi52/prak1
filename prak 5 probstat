        "mean_arrivals_per_hour = mean_arrival_rate * 60  # 60 menit dalam satu jam\n",
        "\n",
        "# Menampilkan hasil\n",
        "print(\"Rata-rata jumlah kedatangan selama periode 1 jam: {:.2f}\".format(mean_arrivals_per_hour))\n"
